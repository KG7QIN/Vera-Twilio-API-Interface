<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<actionList>
		<action>
			<name>InitiateVoiceCall</name>
			<argumentList>
				<argument>
					<name>PhoneNumber</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
				<argument>
					<name>Message</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SendSMSMessage</name>
			<argumentList>
				<argument>
					<name>PhoneNumber</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
				<argument>
					<name>Message</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>CustomVoiceCall</name>
			<argumentList>
				<argument>
					<name>PhoneNumber</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
				<argument>
					<name>Message</name>
					<direction>in</direction>
					<relatedStateVariable>string</relatedStateVariable>
				</argument>
				<argument>
					<name>MsgFlag</name>
					<direction>in</direction>
					<relatedStateVariable>int</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>
